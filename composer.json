{
    "name": "ontherocksoftware/laravelredambergreen",
    "description": "Laravel package to easily update business KPI monitors for your software or service.",
    "license": "MIT",
    "authors": [
        {
            "name": "Heinz Seldte",
            "email": "heinz@otrsw.com",
            "homepage": "https://red-amber.green"
        }
    ],
    "homepage": "https://github.com/ontherocksoftware/laravelredambergreen",
    "keywords": ["Laravel", "LaravelRedAmberGreen"],
    "require": {
        "illuminate/support": "~7|~8",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Ontherocksoftware\\LaravelRedAmberGreen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ontherocksoftware\\LaravelRedAmberGreen\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ontherocksoftware\\LaravelRedAmberGreen\\LaravelRedAmberGreenServiceProvider"
            ],
            "aliases": {
                "LaravelRedAmberGreen": "Ontherocksoftware\\LaravelRedAmberGreen\\Facades\\LaravelRedAmberGreen"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }    
}
